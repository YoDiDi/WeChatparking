<!--pages/information/information.wxml-->
<image src='../../img/black.png'></image>
  <view>

    <text>姓名</text>
    <input bindinput='name' placeholder='请输入真实姓名'></input>
    <text>车牌</text>
    <input bindinput='carID' placeholder='请输入车牌 如:皖A88888' maxlength='7'></input>
  </view>

  <view class='buttonBottom'>
    <button bindtap='submit'>提交</button>
    <button bindtap='exit'>退出</button>
  </view>
